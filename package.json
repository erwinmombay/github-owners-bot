{
  "name": "github-owners-bot",
  "author": "Google Inc.",
  "private": true,
  "version": "0.0.1",
  "repository": "http://github.com/ampproject/github-owners-bot",
  "description": "A web service that suggests approvers for a GitHub pull request based on OWNERS files and enforces reviews by OWNERS as a GitHub status check.",
  "engines": {
    "node": "^8.0.0",
    "yarn": "^1.3.2"
  },
  "scripts": {
    "start": "node dist/app.js",
    "monitor": "nodemon app.js",
    "deploy": "gcloud app deploy app.yaml",
    "test": "ava"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "express-winston": "2.0.0",
    "lodash": "4.17.4",
    "nconf": "0.8.4",
    "request": "2.74.0",
    "winston": "2.2.0",
    "yamljs": "0.2.8"
  },
  "devDependencies": {
    "@google-cloud/debug-agent": "^2.6.0",
    "@google-cloud/trace-agent": "^3.1.1",
    "del": "2.2.2",
    "eslint": "^3.13.1",
    "eslint-plugin-ava": "4.2.0",
    "gulp": "3.9.1",
    "gulp-ava": "0.14.1",
    "gulp-batch": "^1.0.5",
    "gulp-eslint": "3.0.1",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-watch": "4.3.11",
    "lazypipe": "1.0.1",
    "minimist": "1.2.0",
    "sinon": "1.17.6",
    "supertest": "3.0.0"
  },
  "ava": {
    "files": [
      "tests.dist/**/{test,test-*}.js"
    ],
    "concurrency": 5,
    "tap": false,
    "failFast": true
  }
}
